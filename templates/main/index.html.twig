{% extends 'base.html.twig' %}

{% block title %}Quiz Manager!{% endblock %}

{% block body %}

<h1>List of all quizzes</h1>
    <table class='table table-striped'>
        <thead>
            <tr>
                <td>ID</td><td>name</td>
            </tr>
        </thead>
        <tbody>

{% for quiz in quizzes %}
    <tr>
        <td>{{quiz.id}}</td>
        <td><a href="{{ path('start', {id: quiz.id})}}">{{quiz.name}}</a></td>
    </tr>
{% endfor %}

{% endblock %}
